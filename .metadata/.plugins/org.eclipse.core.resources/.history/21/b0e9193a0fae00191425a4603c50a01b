
public class LowerToUpper {
	String s = new String();
	String result = "";
	char ch=' ';
	for(int i = 0;i<s.length();i++)
	{
		if (s.charAt(i) >= 'a' && s.charAt(i) <= 'z') {
			ch = (char) (s.charAt(i) - 32);
		} else {
			ch = (char) (s.charAt(i));
		}
		result = result + ch;
	return;
}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
	
	System.out.println("gtuyybckua");
}
