
public class EmptyOrNot {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String str = "this is a program to check given string empty or not";
		String str1="";
		
		if(str.isEmpty()==true){
			System.out.println("Given str string is empty ");
		}else {
			System.out.println("Given str string is not empty ");	
		}
		if(str1.isEmpty()==true){
			System.out.println("Given str1 string is empty ");
		}else {
			System.out.println("Given str1 string is not empty ");
		}
	}

}
