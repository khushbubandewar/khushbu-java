
public class StringSE {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String str= "This is program  of String start With method";
		if(str.startsWith("This")==true) {
			System.out.print("String Strats with This"+true);
		}else {
			System.out.print("String does not Starts with This "+false);
		}
	}

}
