
public class StringExa1 {
	String name;
	int rollNo;
	StringExa1(String name,int rollNo){
		this.name = name;
		this.rollNo =rollNo;
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		StringExa1 s1 = new StringExa1("xxxxxx",1234);
		StringExa1 s2 = new StringExa1("yyyyy",9876);
 			System.out.println(s1);
 			System.out.println(s2);
	}

}
