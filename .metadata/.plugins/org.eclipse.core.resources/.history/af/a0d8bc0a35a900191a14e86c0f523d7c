
public class ReverseLoopPyramid {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for(int i=5; i>=0; i--) {
			for(int j=5; j<=j-i; j--) {
				System.out.print(" -- ");
				
			}
			for(int k=5; k>=i; k--) {
				System.out.print(" * ");
			}
			System.out.println(" ");
		}
	}

}
