public class HashMapTest {

	private static final String[][] String = null;

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		LinkedList<Employee> employeeList = new LinkedList<Employee>();
		Employee emp1 = new Employee();
		emp1.setName("Alfa");
		emp1.setCity("pune");
		emp1.setId(12356);
		employeeList.add(emp1);
		Map<String, Object> map = new HashMap<String, Object>();
		map.put("employeeList", employeeList);
		Set<Entry<String, Object>> set = map.entrySet();
		// Iterator<Employee> itr = set.iterator();
		// === Employee/Student/College Details==
		//Employee Name = 
		//Employee City = 
		//Employee ID = 
		for (Entry<String, Object> entry : set) {
			System.out.println(entry.getKey() + " " + entry.getValue());
			if(entry.getValue() instanceof  LinkedList) {
				LinkedList<Employee> employeeData = (LinkedList<Employee>)entry.getValue();
				for(Employee emp :employeeData) {
					System.out.println("=== Employee Details==");
					System.out.println(" Employee Name "+emp.getName());
					System.out.println(" Employee  City "+emp.getCity());
					System.out.println(" Employee  ID "+emp.getId());
				}
			}
		}
		
