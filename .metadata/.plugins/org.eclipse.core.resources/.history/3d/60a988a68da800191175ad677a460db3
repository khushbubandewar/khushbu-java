
public class LoopExample {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		// for loop
		for (int i = 1; i <= 10; i++) {
			System.out.println(i);
		}

		// while loop
		int i = 1;
		while (i <= 5) {
			System.out.println(i);
			i++;
		}

		// do while loop
		int i1 = 0;
		do {
			System.out.println(i1);
			i++;
		} while (i <=7);

	}

}
