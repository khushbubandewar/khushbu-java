import java.util.LinkedList;

public class LinkedListDemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		LinkedList l = new LinkedList();
		l.add("theSDFGHJKERTY");
		l.add("qwer");
		l.add("kjhg");
		l.toArray();System.out.println("ADD   "+l);
		l.size();
		System.out.println("SIZE  "+l);
		l.iterator();
		System.out.println("ITERETOR  "+l);
		l.add(2, "eeeeee");
		System.out.println("ADD  "+l);
		l.addLast("m");
		System.out.println("ADDLAST  "+l);
		l.clone();
		System.out.println("CLONE   "+l);
		l.contains(l);
		System.out.println("CONTAIS  "+l);
		l.descendingIterator();
		System.out.println("REVRSE  "+l);
		l.remove("h");
		System.out.println("REMOVE  "+l);
		l.getFirst();
		System.out.println("GETFIRST  "+l);
		l.getLast();
		System.out.println("GETLAST  "+l);
		l.get(3);
		System.out.println("GET  "+l);
		l.hashCode();
		System.out.println("HASHCODE  "+l);
		l.isEmpty();
		System.out.println("EMPTY   "+l);
		l.listIterator();
		System.out.println("ITERATOR  "+l);
		l.notify();
		System.out.println("NOTIFY  "+l);
		l.indexOf(4);
		System.out.println("INDEXOF  "+l);
		
	}

}
